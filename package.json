    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-register": "^6.26.0",
    "bootstrap": "^3.3.0",
    "eslint": "^5.14.0",
    "eslint-config-eslint": "^5.0.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^1.0.0",
    "http-server": "^0.11.1",
    "jasmine-spec-reporter": "^4.2.1",
